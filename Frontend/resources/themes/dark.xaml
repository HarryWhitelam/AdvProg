<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush"    Color="#FF212121"                />
    <SolidColorBrush x:Key="ControlBrush"       Color="#FF5D5D5D"                />
    <SolidColorBrush x:Key="ExpanderBrush"      Color="#FF3A3A3A"                />
    <SolidColorBrush x:Key="BorderBrush"        Color="#FF94A2A2"                />
    <SolidColorBrush x:Key="FontBrush"          Color="White"                    />
    <SolidColorBrush x:Key="ErrorBrush"         Color="Red"                      />
    <SolidColorBrush x:Key="CaretBrush"         Color="White"                    />
    <BitmapImage     x:Key="helpImageSource"        UriSource="/resources/helpInvert.png"       />
    <BitmapImage     x:Key="settingsImageSource"     UriSource="/resources/settingsInvert.png"   />
</ResourceDictionary>