<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="BackgroundBrush"    Color="Black"           />
    <SolidColorBrush x:Key="ControlBrush"       Color="#FF5D5D5D"       />
    <SolidColorBrush x:Key="ExpanderBrush"      Color="#FF3A3A3A"       />
    <SolidColorBrush x:Key="BorderBrush"        Color="Cyan"            />
    <SolidColorBrush x:Key="FontBrush"          Color="Yellow"          />
    <SolidColorBrush x:Key="ErrorBrush"         Color="Cyan"            />
    <SolidColorBrush x:Key="CaretBrush"         Color="White"           />
    
    <BitmapImage     x:Key="helpImageSource"         UriSource="/resources/helpInvert.png"       />
    <BitmapImage     x:Key="settingsImageSource"     UriSource="/resources/settingsInvert.png"   />
</ResourceDictionary>