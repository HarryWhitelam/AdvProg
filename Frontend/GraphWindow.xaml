<Window x:Class="Frontend.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Frontend"
        xmlns:d3="clr-namespace:InteractiveDataDisplay.WPF;assembly=InteractiveDataDisplay.WPF"
        xmlns:DV="clr-namespace:System.Windows.Controls.DataVisualization;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        xmlns:DVC="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        mc:Ignorable="d"
        Title="Baltam" Height="750" Width="800">

    <Grid>
        <Rectangle HorizontalAlignment="Center" Height="780" Margin="10,10,10,10" Stroke="Black" VerticalAlignment="Top" Width="780"/>

        <Border BorderThickness="1" BorderBrush="White" Margin="10">
            <d3:Chart Name="myChart">
                <Grid Name="myGrid"/>
            </d3:Chart>
        </Border>


    </Grid>
</Window>
